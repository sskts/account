{
  "name": "sskts-account",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint",
    "clean": "rimraf ./dst ./doc ./coverage \"npm-debug.log*\" ./.nyc_output",
    "coverage": "nyc mocha \"src/**/*.spec.ts\"",
    "css": "npm run scss && postcss public/css/**/*.css -u autoprefixer -d public/css",
    "scss": "node-sass public/scss --output public/css",
    "start": "node ./dst/bin/www",
    "start:development": "node ./dst/bin/ssl",
    "test": "npm run check && npm run coverage",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" \"src/**/*.ts\"",
    "preversion": "npm run clean && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "deploy:development": "gcloud app deploy ./app.development.gcp.yaml --project sskts-dev",
    "deploy:test": "gcloud app deploy ./app.test.gcp.yaml --project sskts-test",
    "deploy:production": "gcloud app deploy ./app.production.gcp.yaml --project sskts-prod-account --no-promote --version staging"
  },
  "dependencies": {
    "aws-sdk": "^2.220.1",
    "basic-auth": "^2.0.0",
    "bootstrap": "^4.4.1",
    "connect-redis": "^3.3.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-ejs-layouts": "^2.4.0",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "google-libphonenumber": "^3.1.5",
    "http-errors": "~1.6.2",
    "http-status": "^1.0.1",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.2.1",
    "morgan": "^1.9.1",
    "redis": "^3.1.1",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/basic-auth": "^1.1.2",
    "@types/connect-redis": "0.0.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.1",
    "@types/express-ejs-layouts": "^2.3.1",
    "@types/express-flash": "0.0.0",
    "@types/express-session": "^1.15.8",
    "@types/google-libphonenumber": "^7.4.15",
    "@types/http-errors": "^1.6.1",
    "@types/http-status": "^0.2.30",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/mocha": "^5.0.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.17.60",
    "@types/redis": "^2.8.6",
    "@types/request": "^2.47.0",
    "@types/request-promise-native": "^1.0.14",
    "@types/uuid": "^3.4.3",
    "autoprefixer": "^8.3.0",
    "mocha": "^5.1.0",
    "node-sass": "^6.0.1",
    "nyc": "^15.1.0",
    "postcss": "^6.0.21",
    "postcss-cli": "^5.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.5.2"
  }
}
